# Environment Variables for EMTEEGEE Swarm System
# Copy this file to .env and add your actual values

# MongoDB Connection (required)
# Get this from MongoDB Atlas or your MongoDB provider
MONGODB_CONNECTION_STRING=mongodb+srv://username:password@cluster.mongodb.net/emteegee_dev?retryWrites=true&w=majority

# Django Settings (required)
DJANGO_SECRET_KEY=your-secret-key-here-change-this-in-production
DJANGO_DEBUG=True
DJANGO_API_BASE_URL=http://localhost:8001

# Ollama Configuration (optional)
# If running Ollama locally for AI analysis
OLLAMA_HOST=http://localhost:11434

# Swarm Configuration (required for workers)
# For local development, use localhost
# For production, use your server domain/IP
SWARM_SERVER_URL=http://localhost:8001

# Production Settings (for deployment)
# DJANGO_DEBUG=False
# DJANGO_API_BASE_URL=https://yourdomain.com
# SWARM_SERVER_URL=https://yourdomain.com
